{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "EnableLogSampling": true,
  "SamplingConfig": {
    "Environment": "Production",
    "TraceSampleRate": 0.25,
    "MetricSampleRate": 0.5,
    "LogSampling": {
      "Debug": 0.01,
      "Information": 0.1,
      "Warning": 0.5,
      "Error": 1.0
    }
  }
}
